{
  "name": "astar-examples",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@727-ventures/typechain-compiler": "^1.1.4",
    "@727-ventures/typechain-types": "^1.1.2",   
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "jest": "^29.3.1",
    "patch-package": "^8.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.0"
  },
  "scripts": {
    "compile": "typechain-compiler --toolchain nightly",
    "compile:release": "typechain-compiler --release --toolchain nightly",
    "test:typechain": "jest --testPathPattern \".spec.ts$\" --runInBand",
    "test:single": "jest",
    "lint": "prettier --check . && eslint . && cargo check",
    "lint:fix": "prettier --write . && eslint . --fix && cargo fmt --all",
    "deploy": "ts-node scripts/deploy.ts",
    "postinstall": "patch-package"
  },
  "resolutions": {
    "@polkadot/api": "^11.2.1",
    "@polkadot/api-contract": "^11.2.1"
  },
  "devDependencies": {
    "dotenv": "^16.0.3"
  }
}
